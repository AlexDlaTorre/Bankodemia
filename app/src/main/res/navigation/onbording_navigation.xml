<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/onbording_navigation"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.bankodemia.UI.view.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_createAccountFragment2"
            app:destination="@id/createAccountFragment" />
        <action
            android:id="@+id/action_mainFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_mainFragment_to_createAccountFragment"
            app:destination="@id/createAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.bankodemia.ui.view.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/action_loginFragment_to_mobile_navigation"
            app:destination="@id/mobile_navigation"
            app:popUpTo="@id/navigation_home"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/createAccountFragment"
        android:name="com.example.bankodemia.ui.view.CreateAccountFragment"
        android:label="fragment_create_account"
        tools:layout="@layout/fragment_create_account" >
        <action
            android:id="@+id/action_createAccountFragment_to_dataFragment"
            app:destination="@id/dataFragment" />
        <action
            android:id="@+id/action_createAccountFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/dataFragment"
        android:name="com.example.bankodemia.ui.view.DataFragment"
        android:label="fragment_data"
        tools:layout="@layout/fragment_data" >
        <action
            android:id="@+id/action_dataFragment_to_telephoneFragment"
            app:destination="@id/telephoneFragment" />
        <action
            android:id="@+id/action_dataFragment_to_createAccountFragment"
            app:destination="@id/createAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/telephoneFragment"
        android:name="com.example.bankodemia.ui.view.TelephoneFragment"
        android:label="fragment_telephone"
        tools:layout="@layout/fragment_telephone" >
        <action
            android:id="@+id/action_telephoneFragment_to_identityFragment"
            app:destination="@id/identityFragment" />
        <action
            android:id="@+id/action_telephoneFragment_to_dataFragment"
            app:destination="@id/dataFragment" />
    </fragment>
    <fragment
        android:id="@+id/identityFragment"
        android:name="com.example.bankodemia.ui.view.IdentityFragment"
        android:label="fragment_identity"
        tools:layout="@layout/fragment_identity" >
        <action
            android:id="@+id/action_identityFragment_to_identityMenuFragment"
            app:destination="@id/identityMenuFragment" />
        <action
            android:id="@+id/action_identityFragment_to_telephoneFragment"
            app:destination="@id/telephoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/identityMenuFragment"
        android:name="com.example.bankodemia.ui.view.IdentityMenuFragment"
        android:label="fragment_identity_menu"
        tools:layout="@layout/fragment_identity_menu" >
        <action
            android:id="@+id/action_identityMenuFragment_to_ineFragment"
            app:destination="@id/ineFragment" />
        <action
            android:id="@+id/action_identityMenuFragment_to_migratoryFragment"
            app:destination="@id/migratoryFragment" />
        <action
            android:id="@+id/action_identityMenuFragment_to_passportFragment"
            app:destination="@id/passportFragment" />
        <action
            android:id="@+id/action_identityMenuFragment_to_identityFragment"
            app:destination="@id/identityFragment" />
    </fragment>
    <fragment
        android:id="@+id/ineFragment"
        android:name="com.example.bankodemia.ui.view.IneFragment"
        android:label="fragment_ine"
        tools:layout="@layout/fragment_ine" >
        <action
            android:id="@+id/action_ineFragment_to_passwordFragment"
            app:destination="@id/passwordFragment" />
        <action
            android:id="@+id/action_ineFragment_to_identityMenuFragment"
            app:destination="@id/identityMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/passportFragment"
        android:name="com.example.bankodemia.ui.view.PassportFragment"
        android:label="fragment_passport"
        tools:layout="@layout/fragment_passport" >
        <action
            android:id="@+id/action_passportFragment_to_passwordFragment"
            app:destination="@id/passwordFragment" />
        <action
            android:id="@+id/action_passportFragment_to_identityMenuFragment"
            app:destination="@id/identityMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/migratoryFragment"
        android:name="com.example.bankodemia.ui.view.MigratoryFragment"
        android:label="fragment_migratory"
        tools:layout="@layout/fragment_migratory" >
        <action
            android:id="@+id/action_migratoryFragment_to_passwordFragment"
            app:destination="@id/passwordFragment" />
        <action
            android:id="@+id/action_migratoryFragment_to_identityMenuFragment"
            app:destination="@id/identityMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/passwordFragment"
        android:name="com.example.bankodemia.ui.view.PasswordFragment"
        android:label="fragment_password"
        tools:layout="@layout/fragment_password" >
        <action
            android:id="@+id/action_passwordFragment_to_confirmationFragment"
            app:destination="@id/confirmationFragment" />
        <action
            android:id="@+id/action_passwordFragment_to_identityMenuFragment"
            app:destination="@id/identityMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/confirmationFragment"
        android:name="com.example.bankodemia.ui.view.ConfirmationFragment"
        android:label="fragment_confirmation"
        tools:layout="@layout/fragment_confirmation" >
        <action
            android:id="@+id/action_confirmationFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <include app:graph="@navigation/mobile_navigation" />
</navigation>